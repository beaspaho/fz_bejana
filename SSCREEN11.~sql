create TABLE STTM_MASTER_SBS3 
(
CUST_ID varchar2(35),
CUST_TYPE varchar2(35),
CUST_CAT varchar2(55),

AUTH_STAT CHAR(1) DEFAULT 'U',
RECORD_STAT CHAR(2) DEFAULT 'U',
MOD_NO NUMBER (4,0),
CHECKER_DT_STAMP DATE,
ONCE_AUTH CHAR(1)  DEFAULT 'N',
MAKER_ID VARCHAR2(12),
MAKER_DT_STAMP  DATE,
CHECKER_ID VARCHAR2(12),
CONSTRAINT PX_STTM_MASTER_SBS3   PRIMARY KEY (CUST_ID)

);

create TABLE STTM_DETAIL_SBS3 
(
CUST_ID_DET varchar2(35),
--TEMP VARCHAR2(35),
FIELD_DES varchar2(35),
MANDATORY_VALIDATION  varchar2(35),
CONSTRAINT PX_STTM_DETAIL_SBS3  PRIMARY KEY (CUST_ID_DET,FIELD_DES)

);
SELECT * FROM STTM_DETAIL_SBS3;
SELECT * FROM STTM_MASTER_SBS3 ;

