--ss 2

CREATE TABLE STTM_MASTER_SC2BS (
PROD_RESTRICTION varchar2(35),
PROD_CODE varchar2(35),
RELATIONSHIP varchar2(35),
CATEGORY_EX  varchar2(35),
DESCRIPTION_EX varchar2(35),
Group_Status varchar2(435),
AUTH_STAT CHAR(1) DEFAULT 'U',
RECORD_STAT CHAR(2) DEFAULT 'O',
MOD_NO NUMBER (4,0),
CHECKER_DT_STAMP DATE,
ONCE_AUTH CHAR(1)  DEFAULT 'N',
MAKER_ID VARCHAR2(12),
MAKER_DT_STAMP  DATE,
CHECKER_ID VARCHAR2(12),
CONSTRAINT PX_STTM_MASTER_SC2BS  PRIMARY KEY (RELATIONSHIP ,CATEGORY_EX  )

);


CREATE TABLE STTM_DETAIL_SC2BS (
RELATIONSHIP varchar2(35),
CATEGORY_EX  varchar2(35),
PROD_CODE varchar2(35),
ACCO varchar2(35),
AMEX varchar2(35),
CONSTRAINT PX_STTM_DETAIL_SC2BS PRIMARY KEY (RELATIONSHIP ,CATEGORY_EX,PROD_CODE  )




);
--ss test 
create TABLE STTM_DTES
(CUST_CAT varchar2(55),
CUST_ID varchar2(35),
CUST_TYPE varchar2(35),
FIELD_DES varchar2(35),
MANDATORY_VALIDATION  varchar2(35),
CONSTRAINT PX_STTM_DTES  PRIMARY KEY (CUST_TYPE,CUST_ID,FIELD_DES)

);
--ss 3
create TABLE STTM_MASTER_SBS3 
(
CUST_ID varchar2(35),
CUST_TYPE varchar2(35),
CUST_CAT varchar2(55),

AUTH_STAT CHAR(1) DEFAULT 'U',
RECORD_STAT CHAR(2) DEFAULT 'U',
MOD_NO NUMBER (4,0),
CHECKER_DT_STAMP DATE,
ONCE_AUTH CHAR(1)  DEFAULT 'N',
MAKER_ID VARCHAR2(12),
MAKER_DT_STAMP  DATE,
CHECKER_ID VARCHAR2(12),
CONSTRAINT PX_STTM_MASTER_SBS3   PRIMARY KEY (CUST_ID)

);

create TABLE STTM_DETAIL_SBS3 
(
CUST_ID_DET varchar2(35),
--TEMP VARCHAR2(35),
FIELD_DES varchar2(35),
MANDATORY_VALIDATION  varchar2(35),
CONSTRAINT PX_STTM_DETAIL_SBS3  PRIMARY KEY (CUST_ID_DET,FIELD_DES)

);
