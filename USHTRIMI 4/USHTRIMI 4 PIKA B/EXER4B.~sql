    declare 
    
    n varchar2 (5);
    n1 varchar2(5);
    q NUMBER := 10000;
    k varchar2(5);
   
    REZ2 NUMBER;
    
    begin 
       n := 'EUR';
       n1 := 'GBP';
       k := '000';
      REZ2 :=  FN_BS_CONVERT_FCUST (n,q ,k,n1 );
          dbms_output.put_line('REZULTATI I RRUMBULLAKOSUR'|| REZ2);
       
      end; 
